{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug: Gabe_s Debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/test/test-rbtree",
      "args": [],                    
      "stopAtEntry": false,         
      "cwd": "${workspaceFolder}",   
      "environment": [],             
      "externalConsole": false,      
      "preLaunchTask": "Build with test/Makefile",
      "MIMode": "gdb",               
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
